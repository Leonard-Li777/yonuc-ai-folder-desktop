    "electron-log": "^5.4.3",
    "exifr": "^7.1.3",
    "ffmpeg-static": "^5.2.0",
    "libreoffice-convert": "^1.6.0",
    "llamaindex": "^0.1.21",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.542.0",
    "music-metadata": "^11.8.3",
    "node-machine-id": "^1.1.12",
    "path-browserify": "^1.0.1",
    "pdf-poppler": "^0.2.3",
    "pdfjs-dist": "^2.16.105",
    "process": "^0.11.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "react-window": "^2.1.1",
    "setimmediate": "^1.0.5",
