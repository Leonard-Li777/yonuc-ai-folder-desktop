{
  "name": "desktop",
  "productName": "yonuc-ai-folder",
  "version": "1.0.0-alpha.1",
  "description": "智能虚拟文件管理工具是一个基于 AI 的文件管理系统，旨在解决用户文件散乱、命名混乱、多维度归类困难等问题。该工具特别针对压缩文件（如漫画压缩包）的管理，提供智能分类、密码管理、多维度查看和一键整理功能。",
  "main": "out_build/main/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yonuc/yonuc-ai-folder-desktop.git"
  },
  "ai-platform": "ollama",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "start:config": "cross-env FORCE_CONFIG_STAGE=1 electron-vite dev",
    "start:quiet": "cross-env LOG_LEVEL=error electron-vite dev -- --log-categories=ERROR,WARNING",
    "start:debug": "cross-env LOG_LEVEL=debug,info,warn,error electron-vite dev -- --log-categories=ALL,-LLAMA_SERVER",
    "start:info": "cross-env LOG_LEVEL=info,warn,error electron-vite dev -- --log-categories=ALL,-LLAMA_SERVER",
    "start:llama": "cross-env LOG_LEVEL=debug,info,warn,error electron-vite dev -- --log-categories=ALL",
    "start:error": "cross-env LOG_LEVEL=warn,error electron-vite dev -- --log-categories=ALL,-LLAMA_SERVER",
    "start:ai-only": "electron-vite dev -- --log-categories=AI_SERVICE,PROCESS_MANAGER,MODEL_MANAGER",
    "start:analysis": "electron-vite dev -- --log-categories=ANALYSIS_QUEUE,FILE_ANALYSIS,DIMENSION_SERVICE",
    "prebuild": "node scripts/setup-extra-resources.js",
    "package": "node scripts/fix-pnpm-modules.js && pnpm run build:prebuild && electron-vite build && electron-forge package",
    "make": "node scripts/make-installers.js",
    "publish": "pnpm run build:prebuild && electron-vite build && node scripts/wait-for-download.js && electron-forge publish",
    "setup-resources": "node scripts/setup-extra-resources.js",
    "validate-deployment": "node build/extraResources/validate-deployment.js",
    "verify-sharp": "node scripts/verify-sharp.js",
    "verify-native-modules": "node scripts/verify-native-modules.js",
    "install-sharp-cross-platform": "node scripts/install-sharp-cross-platform.js",
    "verify-dimensions": "node scripts/verify-dimension-system.js",
    "verify-build": "node scripts/verify-build-artifacts.js",
    "create-offline-installer": "node scripts/create-offline-installer.js",
    "download-llama": "node scripts/download-llama-cpp-releases.js",
    "fix-modules": "node scripts/fix-pnpm-modules.js",
    "rebuild": "electron-rebuild -f -w better-sqlite3,sharp",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:unit": "vitest tests/unit",
    "test:integration": "vitest tests/integration",
    "test:e2e": "vitest tests/e2e",
    "test:coverage": "vitest --coverage",
    "dev:legacy": "node scripts/dev-runner.mjs",
    "build:sync": "pnpm run build:prebuild && electron-vite build && (robocopy out_build out\\yonuc-ai-folder-win32-x64\\resources\\app\\out_build /E /IS /IT /NC /NS /NP /NFL /NDL || exit 0)",
    "build:prebuild": "node scripts/prebuild-setup.js",
    "build:package": "pnpm run build:prebuild && pnpm run build && electron-forge package",
    "build:dist": "pnpm run build:prebuild && electron-vite build && node scripts/ci-build-forge.js",
    "build:dist:win": "pnpm run build:prebuild && electron-vite build && node scripts/ci-build-forge.js --platform=win32",
    "build:dist:mac": "pnpm run build:prebuild && electron-vite build && node scripts/ci-build-forge.js --platform=darwin",
    "build:dist:linux": "pnpm run build:prebuild && electron-vite build && node scripts/ci-build-forge.js --platform=linux",
    "dist": "pnpm run build:prebuild && electron-vite build && node scripts/ci-build-forge.js",
    "dist:win": "pnpm run build:prebuild && electron-vite build && node scripts/ci-build-forge.js --platform=win32",
    "dist:mac": "pnpm run build:prebuild && electron-vite build && node scripts/ci-build-forge.js --platform=darwin",
    "dist:linux": "pnpm run build:prebuild && electron-vite build && node scripts/ci-build-forge.js --platform=linux",
    "build:make": "pnpm run make",
    "build:publish": "pnpm run build:prebuild && pnpm run build && electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "seaeye",
    "email": "seaeye777@qq.com"
  },
  "license": "MIT",
  "dependencies": {
    "@img/colour": "^1.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.3.3",
